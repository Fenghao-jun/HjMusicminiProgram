<!--components/detail-song-item/index.wxml-->
<view class="detail-song-item">
	<view class="index">{{index}}</view>
	<view class="info">
		<view class="detail-song-name">
			{{item.name}}
		</view>
		<view class="detail-song-info">
			<image class="icon" src="/assets/images/icons/sq_icon.png" mode="widthFix"></image>
			<text>{{item.ar[0].name}}</text>
				<text wx:if="{{item.alia[0]}}"> Â· {{item.alia[0]}}</text>
		</view>
	</view>
</view>

