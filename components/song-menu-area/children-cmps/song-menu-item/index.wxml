<!--components/song-menu-area/children-cmps/song-menu-item/index.wxml-->
<wxs src='../../../../utils/format.wxs' module="format"></wxs>
<view class="song-menu-item">
	<view class="poster">
		<image class="image" src="{{item.coverImgUrl}}" mode="widthFix"></image>
		<text class="playcount">{{format.formatCounter(item.playCount)}}</text>
	</view>
	<view class="bottom">{{item.name}}</view>
</view>